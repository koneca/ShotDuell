<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" type="image/png" href="{{ asset('favicon.ico') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="{{ asset('css/all.css') }}" rel="stylesheet"/>
            <link href="{{ asset('css/jquery-ui.css') }}" rel="stylesheet"/>
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/bootstrap-4.4.1.min.css') }}"/>
        {% endblock stylesheets %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="{{ asset('js/jquery-3.6.0.min.js')}}"></script>
            <script src="{{ asset('js/jquery-ui.js')}}"></script>
            <script type="text/javascript" src="{{ asset('js/bootstrap-4.4.1.min.js') }}"></script>
            <script src="{{ asset('js/all.js')}}"></script>
            <script src="{{ asset('js/chartjs/dist/chart.js')}}"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        
    </body>
</html>
